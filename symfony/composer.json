{
  "name": "snoke/ws-symfony-app",
  "type": "project",
  "repositories": [
    {
      "type": "path",
      "url": "../bundle",
      "options": {
        "symlink": true
      }
    }
  ],
  "require": {
    "php": ">=8.4",
    "symfony/framework-bundle": "^8.0",
    "symfony/http-kernel": "^8.0",
    "symfony/routing": "^8.0",
    "symfony/dependency-injection": "^8.0",
    "symfony/config": "^8.0",
    "symfony/http-foundation": "^8.0",
    "symfony/http-client": "^8.0",
    "symfony/event-dispatcher": "^8.0",
    "symfony/yaml": "^8.0",
    "snoke/ws-bundle": "dev-main",
    "predis/predis": "^2.3",
    "php-amqplib/php-amqplib": "^3.7"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  }
}
